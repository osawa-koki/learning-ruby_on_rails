<h1>投稿一覧</h1>
<table>
  <tr>
    <th>タイトル</th>
    <th>本文</th>
    <th>投稿日時</th>
  </tr>
  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.name %></td>
        <td><%= post.content %></td>
        <td><%= post.created_at %></td>
        <td><%= post.updated_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
